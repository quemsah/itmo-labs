<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE collection [
<!ELEMENT Навыки (Фронтэнд,Дизайн,Бэкэнд,Вспомогательные_инструменты)>
<!ATTLIST Навыки
  xmlns CDATA #FIXED ''>
<!ELEMENT Фронтэнд (степень_осведомленности,CSS,JavaScript)>
<!ATTLIST Фронтэнд
  xmlns CDATA #FIXED ''>
<!ELEMENT Дизайн (степень_осведомленности)+>
<!ATTLIST Дизайн
  xmlns CDATA #FIXED ''>
<!ELEMENT Бэкэнд (Базы_данных,Языки)>
<!ATTLIST Бэкэнд
  xmlns CDATA #FIXED ''>
<!ELEMENT Вспомогательные_инструменты (степень_осведомленности)+>
<!ATTLIST Вспомогательные_инструменты
  xmlns CDATA #FIXED ''>
<!ELEMENT CSS (степень_осведомленности)>
<!ATTLIST CSS
  xmlns CDATA #FIXED ''
  значение NMTOKEN #REQUIRED>
<!ELEMENT JavaScript (степень_осведомленности)+>
<!ATTLIST JavaScript
  xmlns CDATA #FIXED ''
  значение NMTOKEN #REQUIRED>
<!ELEMENT Базы_данных (степень_осведомленности)>
<!ATTLIST Базы_данных
  xmlns CDATA #FIXED ''>
<!ELEMENT Языки (степень_осведомленности)>
<!ATTLIST Языки
  xmlns CDATA #FIXED ''>
<!ELEMENT степень_осведомленности EMPTY>
<!ATTLIST степень_осведомленности
  xmlns CDATA #FIXED ''
  в NMTOKEN #REQUIRED
  значение CDATA #REQUIRED>
]>
<Навыки>
    <Фронтэнд>
        <степень_осведомленности в="HTML" значение="кое-что умею" />
