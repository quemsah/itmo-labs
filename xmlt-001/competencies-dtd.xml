        </CSS>
        <JavaScript значение="чайник">
            <степень_осведомленности в="jQuery" значение="умею обращаться к элементам" />
            <степень_осведомленности в="AJAX" значение="мало что умею, но знаю что такое" />
        </JavaScript>
    </Фронтэнд>
    <Дизайн>
        <степень_осведомленности в="Photoshop" значение="на четверочку" />
        <степень_осведомленности в="CorelDRAW" значение="на троечку" />
